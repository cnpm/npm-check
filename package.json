{
  "name": "cnpm-check",
  "version": "5.0.0",
  "description": "Check for outdated, incorrect, and unused dependencies.",
  "main": "lib",
  "scripts": {
    "lint": "xo",
    "test": "./bin/cli.js",
    "repos": "grunt repos # required for grunt readme to run",
    "readme": "grunt readme"
  },
  "xo": {
    "space": 4,
    "rules": {
      "no-warning-comments": [
        0
      ],
      "global-require": [
        0
      ]
    }
  },
  "bin": {
    "npm-check": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylang/npm-check.git"
  },
  "keywords": [
    "npm",
    "outdated",
    "dependencies",
    "unused",
    "changelog",
    "check",
    "updates",
    "api",
    "interactive",
    "cli",
    "safe",
    "updating",
    "updater",
    "installer",
    "devDependencies"
  ],
  "author": {
    "name": "Dylan Greene",
    "email": "dylang@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dylang/npm-check/issues"
  },
  "homepage": "https://github.com/dylang/npm-check",
  "files": [
    "bin",
    "lib"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "depcheck": "^0.6.1",
    "execa": "^0.2.2",
    "giturl": "^1.0.0",
    "global-modules": "^0.2.0",
    "globby": "^4.0.0",
    "inquirer": "^0.12.0",
    "is-ci": "^1.0.8",
    "lodash": "^4.6.1",
    "meow": "^3.7.0",
    "merge-options": "0.0.64",
    "node-emoji": "^1.0.3",
    "npminstall": "^1.6.1",
    "ora": "^0.2.1",
    "package-json": "^2.0.1",
    "path-exists": "^2.1.0",
    "semver": "^5.0.1",
    "semver-diff": "^2.0.0",
    "text-table": "^0.2.0",
    "update-notifier": "^0.6.3"
  },
  "devDependencies": {
    "grunt": "^0.4.0",
    "grunt-cli": "^1.1.0",
    "grunt-release": "^0.13.0",
    "grunt-templates-dylang": "^1.0.9",
    "load-grunt-tasks": "^3.3.0",
    "xo": "^0.13.0"
  }
}
